{
    "name": "ngsoft/cache",
    "description": "A PSR-6/PSR-16/React cache implementation that supports PHP OPCache, APCu, Doctrine, Illuminate or any PSR-6/16 compatible and can chain them all.",
    "keywords": [
        "phpcache", "psr-6", "psr-16", "cache", "caching", "opcache",
        "react", "promise", "doctrine", "illuminate", "chain", "apcu"
    ],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "suggest": {
        "doctrine/cache": "A popular cache implementation that supports many different drivers such as redis, memcache, apc, mongodb and others.",
        "illuminate/cache": "The Illuminate Cache package.",
        "react/cache": "Async, Promise-based cache interface.",
        "ext-Zend-OPcache": "Can Accelerate php file caching see: https://www.php.net/manual/en/opcache.installation.php"
    },
    "provide": {
        "psr/cache-implementation": "^1.0",
        "psr/simple-cache-implementation": "^1.0"
    },
    "conflict": {
        "illuminate/cache": "<5.8"
    },
    "require-dev": {
        "react/cache": ">=1.1",
        "doctrine/cache": "^1.10",
        "illuminate/cache": ">=5.8",
        "symfony/cache": ">=5.2"

    },
    "require": {
        "php": ">=7.4",
        "ngsoft/tools": "^2",
        "psr/cache": "^1",
        "psr/simple-cache": "^1",
        "guzzlehttp/promises": ">=1.4"

    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\": "src/"
        },
        "files": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
