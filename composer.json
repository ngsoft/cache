{
    "name": "ngsoft/cache",
    "description": "A PSR-6/PSR-16/React/Laravel/Doctrine cache implementation that supports OPCache, APCu, Doctrine, Illuminate or any PSR-6/16 implementations and can chain them all.",
    "keywords": [
        "psr-6", "psr-16", "cache", "caching", "opcache", "phpfilecache",
        "react", "promise", "doctrine", "illuminate", "laravel", "chain", "apcu"
    ],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "suggest": {
        "doctrine/cache": "A popular cache implementation that supports many different drivers such as redis, memcache, apc, mongodb and others.",
        "illuminate/cache": "The Illuminate Cache package.",
        "react/cache": "Async, Promise-based cache interface.",
        "ext-Zend-OPcache": "Can Accelerate php file caching see: https://www.php.net/manual/en/opcache.installation.php"
    },
    "provide": {
        "psr/cache-implementation": "^3.0",
        "psr/simple-cache-implementation": "^3.0"
    },
    "conflict": {
        "illuminate/cache": "<9.0.0",
        "react/cache": "<1.1",
        "doctrine/cache": "<1.10.1",
        "symfony/cache": "<6.0",
        "ngsoft/tools": "<3"
    },
    "require-dev": {
        "react/cache": ">=1.1",
        "doctrine/cache": ">=1.10.1,<2.0",
        "illuminate/cache": ">=9.0",
        "symfony/cache": ">=6.0"
    },
    "require": {
        "php": ">=8.1",
        "ngsoft/tools": "^3",
        "psr/cache": "^3",
        "psr/simple-cache": "^3",
        "psr/log": "^1|^2|^3",
        "symfony/var-exporter": ">=6.1.0"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\": "src/"
        },
        "files": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
