{
    "name": "ngsoft/cache",
    "description": "A PSR-6/PSR-16 Cache Implementation .",
    "keywords": ["phpcache", "psr-6", "psr-16", "cache", "caching", "opcache", "tags"],
    "license": "MIT",
    "authors": [{
            "name": "Aymeric Anger",
            "role": "Developer",
            "homepage": "https://github.com/ngsoft"
        }],
    "suggest": {
        "doctrine/cache": "A Set of cache adapters that can be loaded using my library.",
        "ext-Zend-OPcache": "Can Accelerate php file caching see: https://www.php.net/manual/en/opcache.installation.php"
    },
    "provide": {
        "psr/cache-implementation": "^1.0",
        "psr/simple-cache-implementation": "^1.0"
    },
    "require-dev": {
        "doctrine/cache": "^1.10"
    },
    "require": {
        "php": ">=7.4",
        "ngsoft/tools": "^2",
        "psr/cache": "^1",
        "psr/simple-cache": "^1",
        "cache/tag-interop": "^1",
        "symfony/cache-contracts": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "NGSOFT\\": "src/"
        },
        "files": []
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
